{
  "name": "template-ts",
  "version": "0.1.0",
  "description": "TypeScript project template with modern tooling and best practices",
  "keywords": [
    "boilerplate",
    "template",
    "typescript"
  ],
  "license": "MIT",
  "author": "",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsgo -p tsconfig.json",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "oxlint src",
    "format": "oxfmt src",
    "format:check": "oxfmt --check src",
    "type-check": "tsgo --noEmit -p tsconfig.json",
    "clean": "rm -rf dist",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm build && changeset publish",
    "prepare": "husky"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.2",
    "dotenv-expand": "12.0.3",
    "pino": "^10.1.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@types/node": "^25.0.3",
    "@typescript/native-preview": "7.0.0-dev.20251222.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "oxfmt": "^0.20.0",
    "oxlint": "^1.34.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.0"
}
